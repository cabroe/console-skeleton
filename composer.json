{
  "name": "hmazter/console-skeleton",
  "description": "Skeleton for a console application with DI-container and yaml-config",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Kristoffer HÃ¶gberg",
      "email": "krihog@gmail.com",
      "homepage": "https://hmazter.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/config": "^5.0",
    "symfony/console": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "clue/phar-composer": "^1.1",
    "phpunit/phpunit": "^8.2",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "bin": ["./app"],
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "build": "./vendor/bin/phar-composer build"
  },
  "config": {
    "sort-packages": true
  }
}
